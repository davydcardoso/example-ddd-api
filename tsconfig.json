{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "./dist",
    "strict": false,
    "strictPropertyInitialization": false,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "@modules/*":["./src/modules/*"],
      "@shared/*":["./src/shared/*"],
      "@config/*":["./src/config/*"],
      "@utils/*":["./src/utils/*"],
      "@core/*":["./src/core/*"]
    },
  }
}
